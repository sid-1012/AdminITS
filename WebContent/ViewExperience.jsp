                                <div class="x_content">

                                    <table class="table table-bordered">
                                        
                                            <tr>
                                                <th>#</th>
                                                <th>First Name</th>
                                                <th>Last Name</th>
                                                <th>Username</th>
                                                <th>Work Experience</th>
                                                <th>Current Employer</th>
                                                <th>Contact Person</th>
                                                <th>Contact Number</th>
                                                <th>Email</th>
                                                <th>Current Salary</th>
                                                <th>Reason for Leaving</th>
                                            </tr>
                                        	 <tr>
                                             <% 
						Connection connection = DbUtil.getCon();
                        PreparedStatement ps = connection.prepareStatement("SELECT signup.*, experience.* FROM signup JOIN experience ON signup.applicantID = experience.applicantID and flag = 0");
						ResultSet rs = ps.executeQuery();
						int k = 1;
						while(rs.next())
						{
							
						%>
                                        	 <tr>
                                                <th scope="row"><%out.print(k);%></th>
                                                <td><%= rs.getString("firstName")%></td>
                                                <td><%= rs.getString("lastName")%></td>
                                                <td><%= rs.getString("username")%></td>
                                                <td><%= rs.getString("workExperience")%></td>
                                                <td><%= rs.getString("currentEmployer")%></td>
                                                <td><%= rs.getString("contactPerson")%></td>
                                                <td><%= rs.getString("contactNumber")%></td>
                                                <td><%= rs.getString("experience.email")%></td>
                                                <td><%= rs.getString("currentSalary")%></td>
                                                <td><%= rs.getString("reasonForLeaving")%></td>
                                            </tr>
                         <% k++; } %>
                                             </tr>
                                                                                    
                                    </table>

